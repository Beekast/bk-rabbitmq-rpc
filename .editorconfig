# http://editorconfig.org
root = true

[*]
end_of_line = lf
insert_final_newline = true
indent_style = tab
indent_size = 4
charset = utf-8
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[{package.json,bower.json,*jsbeautifyrc,*bowerrc}]
indent-style = space
indent-size = 2
